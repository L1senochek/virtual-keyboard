{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "single", { "allowTemplateLiterals": true }],
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
    }
}
